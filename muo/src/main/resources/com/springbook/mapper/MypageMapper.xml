<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
<mapper namespace="com.springbook.mapper.MypageMapper">  
  
	<select id="getMemberInfo" resultType="com.springbook.domain.MemberVO">
		select mb_nickname, mb_grade, mb_point, mb_eventNum from member where mb_id = #{mb_id}
	</select>
	

</mapper>